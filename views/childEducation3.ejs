<html>

<head>
	<title>Babyhub</title>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
	<link rel="stylesheet" href="/assets/css/forum.css" />
	<link rel="icon" href="/images/bear.png" type="image/ico" />
	<link href="/assets/css/bootstrap.min.css" rel="stylesheet">
	<link href="/assets/css/style.css" rel="stylesheet">

	<!-- addforum按鈕 -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
	<script type="text/javascript">
		// 當頁面打開後就顯示
		$(function () {
			$('#addforum').show();
		});
		// gotop按鈕
		// 當頁面打開後就顯示
		$(function () {
			$('#gotop').show();
			/* 按下GoTop按鈕時的事件 */
			$('#gotop').click(function () {
				$('html,body').animate({ scrollTop: 0 }, 'slow');   /* 返回到最頂上 */
				return false;
			});
		});
	</script>
</head>

<body class="is-preload">

	<!-- Wrapper -->
	<div id="wrapper2">

		<!-- Header -->
		<header id="header">
			<a href="/homepageMan">
				<img class="textlogopic" src="/images/textlogo.png">
			</a>
			<nav class="main">
				<ul>
					<li class="profile" id="profile">
						<span>管理者模式</span>
					</li>
					<li class="menu">
						<a class="fa-bars" href="#menu">Menu</a>
					</li>
				</ul>
			</nav>
		</header>
		<!-- Menu -->
		<section id="menu">

			<!-- Links -->
			<section>
				<ul class="links">
					<li>
						<a href="/childEducation3">
							<h3>小孩教育</h3>
							<p>提供小孩教育的知識。</p>
						</a>
					</li>
					<li>
						<a href="/pregnancyKnowledge3">
							<h3>孕期知識</h3>
							<p>孕期知識、胎教等。</p>
						</a>
					</li>
					<li>
						<a href="/forum3">
							<h3>討論區</h3>
							<p>跟媽媽們討論各種知識。</p>
						</a>
					</li>
				</ul>
			</section>
			<!-- Actions -->
			<section>
				<ul class="actions stacked">
					<li>
						<a href="/mlogout" class="button large fit">登出 Logout</a>
					</li>
				</ul>
			</section>
			<!-- Footer -->
			<section id="footer">
				<ul class="icons">
					<li><a href="mailto:10456015@ntub.edu.tw" class="fa-envelope"><span class="label">Email</span></a>
					</li>
					<li><a href="https://github.com/BabyhubNTUB" class="fa-github-square"><span
								class="label">github</span></a></li>
				</ul>
				<p class="copyright">Copyright&copy;NTUB IMD 108509</p>
			</section>

		</section>
		<!-- Main -->
		<div class="row1">
			<div class="pagetagtop">
				<div class="row">
					<div class="col-md-6">
						<h1 style="padding-top:2%;">小孩教育</h1>
					</div>
					<div class="col-md-6">
						<!-- Search -->
						<section>
							<form class="search" method="get" action="/searchchild">
								<input type="number" name="check" value="3" hidden />
								<input type="text" name="keyword" style="padding-left: 20%;" />
								<button type="submit"
									style="width:80px; height:40px; text-align: center; padding: 5px; font-family:'Noto Sans TC', sans-serif; font-weight:normal; margin-right:0; ">搜尋</button>
							</form>
						</section>
					</div>
				</div>
			</div>
			<div class="col-md-12">
				<% for(var i=0; i<items.length; i++) {%>
				<div class="Box">
					<a href="/achildEducation3/<%= items[i].serno %>">
						<strong><%= items[i].title %></strong>
					</a>
				</div>
				<% } %>
			</div>
		</div>

	</div>

	<!-- addforum按鈕 -->
	<a href="/addEducation" id="addforum">
		<i class="fa fa-pencil"></i>
	</a>
	<!-- gotop按鈕 -->
	<a id="gotop">
		<i class="fa fa-angle-up"></i>
	</a>


	<!-- Scripts -->
	<script src="/assets/js/jquery.min.js"></script>
	<script src="/assets/js/browser.min.js"></script>
	<script src="/assets/js/breakpoints.min.js"></script>
	<script src="/assets/js/util.js"></script>
	<script src="/assets/js/main.js"></script>
	<script src="/assets/js/bootstrap.min.js"></script>
	<script src="/assets/js/scripts.js"></script>

</body>

</html>